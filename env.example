# GoTo Connect SMS Sender - Environment Variables
# Copy this file to .env and fill in your actual values

# Redis Configuration
REDIS_URL=redis://localhost:6379

# GoTo Connect OAuth Configuration
# Get these from https://developer.goto.com/
OAUTH_SERVICE_URL=https://authentication.logmeininc.com
OAUTH_CLIENT_ID=your-client-id-here
OAUTH_CLIENT_SECRET=your-client-secret-here

# OAuth Redirect URI
# For local development: http://localhost:8080/login/oauth2/code/goto
# For production: https://your-domain.com/login/oauth2/code/goto
OAUTH_REDIRECT_URI=http://localhost:8080/login/oauth2/code/goto

# GoTo Connect Account Key
# This is your GoTo Connect account identifier
GOTO_ACCOUNT_KEY=your-goto-account-key-here 